{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "google": "writable",
    "firebase": "readonly",
    "ActiveXObject": "readonly"
  },
  "env": {
    "browser": true
  }
}