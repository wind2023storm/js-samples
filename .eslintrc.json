{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "2018",
    "sourceType": "module"
  },
  "globals": {
    "google": "writable",
    "deck": "readonly",
    "ActiveXObject": "readonly",
    "mdc": "readonly",
    "Split": "readonly",
    "MarkerClusterer": "readonly"
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": 1,
    "no-var": 2,
    "padding-line-between-statements": [
      1,
      {
        "blankLine": "always",
        "prev": ["*"],
        "next": [
          "if",
          "block",
          "block-like",
          "switch",
          "class",
          "function",
          "try",
          "for",
          "do",
          "while",
          "with"
        ]
      }
    ],
    "prefer-arrow-callback": 2
  }
}
